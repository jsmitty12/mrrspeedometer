{
  "name": "mrrspeedometer",
  "version": "0.2.0",
  "description": "Model Railroad Speedometer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsmitty12/mrrspeedometer.git"
  },
  "author": "Joshua Smith <jsmith@jssm.com>",
  "license": "AGPL-3.0+",
  "bugs": {
    "url": "https://github.com/jsmitty12/mrrspeedometer/issues"
  },
  "homepage": "https://github.com/jsmitty12/mrrspeedometer#readme",
  "jest": {
    "testEnvironment": "node",
    "testURL": "http://localhost:4300/",
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/__mocks__/image.js",
      "^shared/(.*)": "<rootDir>/src/shared/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "deploy": "firebase deploy",
    "test": "jest",
    "test:watch": "jest --verbose --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "firebase": "firebase"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.13",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~9.1.13",
    "@angular/compiler": "~9.1.13",
    "@angular/core": "~9.1.13",
    "@angular/forms": "~9.1.13",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~9.1.13",
    "@angular/platform-browser-dynamic": "~9.1.13",
    "@angular/router": "~9.1.13",
    "@angular/service-worker": "^9.1.13",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.6.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular/cli": "^9.1.15",
    "@angular/compiler-cli": "~9.1.13",
    "@angular/language-service": "~9.1.13",
    "@types/jest": "^21.1.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "firebase-tools": "^4.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.0.0",
    "jest-preset-angular": "^5.0.0",
    "jest-trx-results-processor": "^0.0.5",
    "jest-zone-patch": "^0.0.8",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "ts-mockito": "^2.6.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  }
}
