{
    "name": "mrrspeedometer",
    "version": "0.1.0",
    "description": "Model Railroad Speedometer",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jsmitty12/mrrspeedometer.git"
    },
    "author": "Joshua Smith <jsmith@jssm.com>",
    "license": "AGPL-3.0+",
    "bugs": {
        "url": "https://github.com/jsmitty12/mrrspeedometer/issues"
    },
    "homepage": "https://github.com/jsmitty12/mrrspeedometer#readme",
    "jest": {
        "testURL": "http://localhost:4300/",
        "preset": "jest-preset-angular",
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png)$": "<rootDir>/__mocks__/image.js",
            "^shared/(.*)": "<rootDir>/src/shared/$1"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/cypress/"
        ]
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "deploy": "firebase deploy",
        "test": "jest",
        "test:watch": "jest --verbose --watch",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "firebase": "firebase"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.0.3",
        "@angular/common": "^6.0.3",
        "@angular/compiler": "^6.0.3",
        "@angular/core": "^6.0.3",
        "@angular/forms": "^6.0.3",
        "@angular/http": "^6.0.3",
        "@angular/platform-browser": "^6.0.3",
        "@angular/platform-browser-dynamic": "^6.0.3",
        "@angular/pwa": "^0.7.0-beta.1",
        "@angular/router": "^6.0.3",
        "@angular/service-worker": "^6.0.3",
        "core-js": "^2.4.1",
        "primeicons": "^1.0.0-beta.10",
        "primeng": "^6.0.0",
        "rxjs": "^6.2.0",
        "rxjs-compat": "^6.0.0-rc.0",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.7.0",
        "@angular/cli": "^6.0.7",
        "@angular/compiler-cli": "^6.0.3",
        "@angular/language-service": "^6.0.3",
        "@types/jest": "^21.1.1",
        "@types/node": "^8.10.17",
        "codelyzer": "~4.2.1",
        "firebase-tools": "^3.17.7",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "jest": "^21.2.1",
        "jest-preset-angular": "^4.0.0-alpha.1",
        "jest-trx-results-processor": "^0.0.5",
        "jest-zone-patch": "^0.0.8",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.0",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.3.0",
        "ts-node": "~5.0.1",
        "tslint": "~5.9.1",
        "typescript": "~2.7.2"
    }
}
